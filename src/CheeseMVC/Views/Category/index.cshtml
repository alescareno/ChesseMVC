@model IList<CheeseMVC.Models.CheeseCategory>

@if (Model.Count == 0)
{
    <h1>Add Category</h1>
}

@foreach (var category in Model)
{
    <ul>
        <lil>@category.Name</lil>
    </ul>
        <p>
            <a asp-controller="Category" asp-action="Add">Add Category</a>
        </p>
}
